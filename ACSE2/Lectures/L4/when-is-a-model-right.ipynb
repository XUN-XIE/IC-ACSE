    "ax2.legend(loc='best', fontsize=13)\n",
    "ax2.set_xlabel('$x$', fontsize=14)\n",
    "ax2.set_title('Difference between functions', fontsize=14)\n",
    "\n",
    "ax3.plot(x, (func_1(x) - func_2(x))**2, 'k', label = 'Squared difference $(f(x)-g(x))^2$')\n",

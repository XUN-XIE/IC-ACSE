    "ax2.set_xlabel('$\\Delta t$', fontsize=14)\n",
    "ax2.set_ylabel('Error', fontsize=14)\n",
    "ax2.set_title('Forward Euler convergence plot (RMS)', fontsize=16)\n",
    "ax2.grid(True)\n",
